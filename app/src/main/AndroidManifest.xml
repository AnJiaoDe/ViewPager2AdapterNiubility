<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cy.loopviewpageradapter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.cy.loopviewpageradapter_.ViewPager2LoopAdapterActivity"></activity>
        <activity android:name="com.cy.loopviewpageradapter_.ViewPager2AdapterActivity" />
        <activity android:name="com.cy.loopviewpageradapter_.ViewPagerAdapterActivity" />
        <activity android:name="com.cy.loopviewpageradapter_.ViewPagerLoopAdapterActivity" />
        <activity android:name="com.cy.loopviewpageradapter_.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>